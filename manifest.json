{
    "manifest_version": 3,
    "name": "YT Watcher",
    "version": "1.0",
    "description": "Ajoute une image sur le côté gauche de YouTube.",
    "permissions": ["scripting"],
    "host_permissions": ["https://www.youtube.com/*"],
    "content_scripts": [{
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"]}],
    "icon": {"sus": "sus.png"}
  }
  